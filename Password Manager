#The code:
class Manage:
    def __init__(self,Username,Password):
        self.Username = Username
        self.Password = Password
    def print_config(self):
        print("Username:",self.Username,
               "Password:",self.Password)

#The input:
Instagram = Manage("Ipykcan_","123455")
Facebook = Manage("ipykcanbook","1234face")
GitHub = Manage("ipykcan_","github123")

#for output use xyz.print_config()
